<?xml version="1.0" encoding="UTF-8"?>
<ProcessDescription processVersion="2" >
  <Identifier>ProjectList</Identifier>
  <Title>获取工程列表</Title>
  <Abstract>获取所有工程的描述列表，以XML字符串形式返回</Abstract>
  <DataInputs>
	<Input>
      <Identifier>BaseDir</Identifier>
      <Title>文件夹</Title>
      <Abstract>文件夹</Abstract>
      <DataType>string</DataType>
	  <Option>false</Option>
    </Input>
  </DataInputs>
  <ProcessOutputs>
	<Output>
      <Identifier>Projects</Identifier>
      <Title>工程列表</Title>
      <Abstract>所有工程的名称列表</Abstract>
      <DataType>string</DataType>
    </Output>
  </ProcessOutputs>
</ProcessDescription>