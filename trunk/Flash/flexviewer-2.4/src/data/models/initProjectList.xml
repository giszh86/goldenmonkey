<?xml version = "1.0" encoding = "UTF-8" ?> 
<!--
  domain(type)={"FFD","WPS","GPW"}
--> 
<process type="FFD" stepByStep="false">
	
	<head>
		<Identifier>initProjectList</Identifier>
		<Title>初始化工程列表</Title>
		<Abstract>获取所有工程的名称列表，并填充到工程列表控件中</Abstract>	
		<DataInputs>		
			<Input>
				<Identifier>Name</Identifier>
				<Title>模型名称</Title>
				<Abstract>给定的模型名称</Abstract>
				<DataType>string</DataType>
				<Option>false</Option>
				<Reference id="2" name="Name" />
			</Input>
			<Input>
				<Identifier>Argument</Identifier>
				<Title>模型参数</Title>
				<Abstract>给定模型的参数</Abstract>
				<DataType>string</DataType>
				<Option>false</Option>
				<Reference id="2" name="Argument" />
			</Input>	
			<Input>
				<Identifier>BasicURL</Identifier>
				<Title>服务基址</Title>
				<Abstract>服务基址</Abstract>
				<DataType>string</DataType>
				<Option>false</Option>
				<Reference id="3" name="BasicURL" />
			</Input>
			<Input>
				<Identifier>ServiceURL</Identifier>
				<Title>服务名</Title>
				<Abstract>服务名</Abstract>
				<DataType>string</DataType>
				<Option>false</Option>
				<Reference id="3" name="ServiceURL" />
			</Input>			
		</DataInputs>
		<ProcessOutputs>	
			<Output>
				<Identifier>ProjectArray</Identifier>
				<Title>工程数组</Title>
				<Abstract>工程名称数组</Abstract>
				<DataType>ArrayCollection</DataType>
			</Output>		
	    </ProcessOutputs>	 
	</head>
	
	

  <flow>   
    <start id="1" />   
    <operation name="CreateRequestContent" id="2">
      <inputs>  
		<input name="Name" type="String" />
		<input name="Argument" type="String" />
      </inputs>
      <outputs>  
		<output name="Content" type="String" />
      </outputs>
    </operation>
	<invoke id="3" >     
      <inputs>
	    <input name="BasicURL" type="String" />
		<input name="ServiceURL" type="String" />
		<input name="Request" type="String" />
      </inputs>
      <outputs>
		<output name="Respond" type="Object"/>
      </outputs>
    </invoke>
    <operation name="CreateProjectArray" id="4">
      <inputs>  
		<input name="Projects" type="ModelResult" />	  
      </inputs>
      <outputs>  
		<output name="ProjectArray" type="ArrayCollection" />
      </outputs>
    </operation>  
    <end id="5" />

    <links>
      <link from="1" to="2" />
      <link from="2" to="3" >
	   <assign>
            <from>Content</from>
            <to>Request</to>
        </assign>
	  </link>
      <link from="3" to="4" >
		<assign>
            <from>Respond</from>
            <to>Projects</to>
        </assign>
	  </link>
	  <link from="4" to="5" />
    </links>
	
	<reply name="CreateProjectArray" kind="" operation="CreateProjectArray" id="4" variable="ProjectArray" /> 
  </flow>
</process>